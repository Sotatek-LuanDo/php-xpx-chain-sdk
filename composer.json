{
    "name": "proximax/php-xpx-chain-sdk",
    "description": "Composer Package for PHP, Wrapper to use the ProximaX Sirius-Chain REST API and SDK.",
    "keywords": ["proximax", "cryptocurrency", "api", "sdk", "restful", "proximax-php", "php-proximax"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gregory Saive",
            "email": "greg@evias.be"
        }
    ],
    "repositories": [],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0.33|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.0.33|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "guzzlehttp/guzzle": "5.3.1|6.*",
        "mdanter/ecc": "0.3.2|0.4.5|0.5.0",
        "paragonie/sodium_compat": "~1.4.0",
        "christian-riesen/base32": "~1.3.1",
        "kornrunner/keccak": "dev-master",
        "google/flatbuffers": "^1.11"
    },
    "require-dev": {
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "^4.8|^5.0|^6.4",
        "phpspec/phpspec": "~2.1",
        "phpdocumentor/phpdocumentor" : "~2.8.0",
        "evert/phpdoc-md" : "~0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Proximax\\": "src/",
            "Proximax\\API\\": "src/api/",
            "Catapult\\Buffers\\": "src/buffers/",
            "Google\\FlatBuffers\\": "php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proximax\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
